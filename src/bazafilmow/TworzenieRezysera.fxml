<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bazafilmow.TworzenieRezyseraController">
   <children>
      <GridPane AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="160.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="250.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Imie" />
            <Label text="Nazwisko" GridPane.rowIndex="1" />
            <Label text="Narodowość" GridPane.rowIndex="2" />
            <Label text="Data Urodzenia" GridPane.rowIndex="3" />
            <TextField fx:id="imie" GridPane.columnIndex="1" />
            <TextField fx:id="nazwisko" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="narod" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="urodz" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Button fx:id="OKButton" defaultButton="true" layoutX="99.0" layoutY="280.0" maxHeight="-Infinity" mnemonicParsing="false" onAction="#handleOKButton" prefHeight="20.0" prefWidth="100.0" text="OK" AnchorPane.bottomAnchor="30.0" AnchorPane.rightAnchor="250.0" />
      <Button fx:id="CloseButton" layoutX="256.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="20.0" prefWidth="100.0" text="Cancel" AnchorPane.bottomAnchor="30.0" AnchorPane.rightAnchor="100.0" />
   </children>
</AnchorPane>
